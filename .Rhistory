nrmse(real_rates_test, bell_rate_test)
),
rae = c(
rae(real_rates_test, bell_rate_test)
),
rmsle = c(
rmsle(real_rates_test, bell_rate_test)
),
rse = c(
rse(real_rates_test, bell_rate_test)
),
cor = c(
cor(real_rates_test, bell_rate_test)
)
)
