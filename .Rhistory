f(ano, model = 'rw1', constr = T) +
f(idArea, model = 'bym2', graph = am_path) +
f(idMes, model = 'rw1') +
f(idInteraction, model = 'iid') + offset(log(populacao))
#bell----------------------------------------------------------------------
